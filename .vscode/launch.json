    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch Simulator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/exe/frcUserProgramSim/frcUserProgramSim.exe",
            "args": ["--start","0","--auto","60","--oper","0", "--test", "0", "--simprop", "pos=17.0,115.0","--simfile","SIM","--robotfile","ROBOT", "--simprop", "autovalue=0.7"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\cygwin64\\bin\\gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
		},
        {
            "name": "(gdb) Launch Robot",
            "type": "cppdbg",
            "request": "launch",
            "program": "C:/cygwin64/home/ButchGriffin/src/new/master/build/exe/frcUserProgram",
            "args": [],
            "stopAtEntry": true,
            "targetArchitecture": "arm",
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/cygwin64/home/ButchGriffin/src/robotdev/external/frc/bin/arm-frc-linux-gnueabi-gdb.exe",
            "customLaunchSetupCommands": [
                {
                    "text": "file C:/cygwin64/home/ButchGriffin/src/new/master/build/exe/frcUserProgram",
                    "description": "load symbols",
                    "ignoreFailures": false
				},
                {
                    "text": "target remote 10.14.25.2:3333",
                    "description": "connect to remote target",
                    "ignoreFailures": false
                },
            ],
			"logging":{"engineLogging": true, "programOutput": true}  
        }		
    ]
}
