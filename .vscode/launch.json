{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "wpilib",
      "name": "WPILib Desktop Debug",
      "request": "launch",
      "desktop": true,
    },
    {
      "type": "wpilib",
      "name": "WPILib roboRIO Debug",
      "request": "launch",
      "desktop": false,
    },
    {
      "name": "CygWin Ranseur Simulator Launch",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/makebuild/SIMULATOR/Debug/ranseur/ranseur.exe",
      "stopAtEntry": false,
      "environment": [],
      "args" : [
        "--test","0.0",
        "--start", "0.0",
        "--auto", "15.0",
        "--oper","120.0",
        "--events","robots/ranseur/sims/automode0.sti"
      ],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },
    {
      "name": "CygWin Phoenix Simulator Launch",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/makebuild/SIMULATOR/Debug/phoenix/phoenix.exe",
      "stopAtEntry": false,
      "environment": [],
      "args" : [
        "--test","0.0",
        "--start", "2.0",
        "--auto", "15.0",
        "--oper","0.0",
        "--events","robots/phoenix/sims/automode0.sti"
      ],      
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },  
    {
        "name": "CygWin MotorController Simulator Launch",
        "type": "cppdbg",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "program": "${workspaceFolder}/makebuild/SIMULATOR/Debug/motorcontroller/motorcontroller.exe",
        "stopAtEntry": false,
        "environment": [],
        "args" : [
          "--test","0.0",
          "--start", "1.0",
          "--auto", "0.0",
          "--oper","15.0",
        ],      
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
      },  
      {
        "name": "Phoenix SIM2 Launch",
        "type": "cppdbg",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "program": "${workspaceFolder}/build/SIM2/Debug/phoenix/phoenix.exe",
        "stopAtEntry": false,
        "environment": [],
        "args" : [      
          "--props","robots/phoenix/sim2/robot.json",
          "--events","robots/phoenix/sim2/simulations/automode0.json"
        ],      
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
      },        
      {
        "name": "Droid SIM2 Launch",
        "type": "cppdbg",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "program": "${workspaceFolder}/build/SIM2/Debug/droid/droid.exe",
        "stopAtEntry": false,
        "environment": [],
        "args" : [      
          "--props","robots/droid/sim2/droid.json",
          "--events","robots/droid/sim2/sims/collect3_shoot.json",
          "--debug","5"
        ],      
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
        "setupCommands": [
            {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
      },              
    {
      "name": "CygWin Unit Test Launch",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/xerolibs/xeromisctest/xeromisctest.exe",
      "stopAtEntry": false,
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/cygwin64/bin/gdb.exe",
      "setupCommands": [
          {
              "description": "Enable pretty-printing for gdb",
              "text": "-enable-pretty-printing",
              "ignoreFailures": true
          }
      ]
    },    
    {
      "name": "LLDB Simulator Launch",
      "type": "lldb-vscode",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/makebuild/SIMULATOR/Debug/ranseur/ranseur",
      "stopOnEntry": false,
      "args": [
          "--test","0.0",
          "--start", "0.0",
          "--auto", "15.0",
          "--oper","0.0",
          "--events","robots/ranseur/sims/automode0.sti"
        ]
      }
  ]
}
